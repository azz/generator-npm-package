{
  "name": "@azz/generator-npm-package",
  "version": "0.0.0",
  "description": "Generate an npm package",
  "author": "Lucas Azzola <@azz>",
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": [
    "yo",
    "generator",
    "npm",
    "babel",
    "prettier",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "figlet": "^1.2.0",
    "globby": "^7.1.1",
    "read-pkg": "^3.0.0",
    "repo-exists": "^2.0.0",
    "yeoman-generator": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": ["error"]
    },
    "plugins": ["prettier"]
  },
  "repository": "azz/generator-npm-package",
  "license": "MIT"
}
